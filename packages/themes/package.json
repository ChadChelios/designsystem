{
  "name": "@unitednations/themes",
  "description": "Themes for applying color in the Carbon Design System",
  "version": "10.40.12",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "module": "es/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/carbon-design-system/carbon.git",
    "directory": "packages/themes"
  },
  "bugs": "https://github.com/carbon-design-system/carbon/issues",
  "keywords": [
    "ibm",
    "elements",
    "carbon",
    "carbon-elements",
    "carbon-design-system",
    "components",
    "react"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "ci-check": "wfp-cli check \"scss/**/*.scss\" -i \"**/generated/**\" -i \"**/compat/**\"",
    "clean": "rimraf es lib umd scss/generated",
    "extract:colors": "node tasks/extractFigma.js",
    "build": "yarn clean && wfp-cli bundle src/index.js --name CarbonThemes && babel-node --presets '@babel/preset-env' tasks/build.js && wfp-cli check \"scss/*.scss\""
  },
  "dependencies": {
    "@carbon/colors": "^10.30.0",
    "@unitednations/layout": "^10.26.13",
    "@unitednations/scss-generator": "^10.13.6",
    "@unitednations/type": "^10.30.14",
    "color": "^3.1.2"
  },
  "devDependencies": {
    "@babel/node": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "@carbon/cli-reporter": "^10.5.0",
    "@carbon/test-utils": "^10.3.0",
    "@unitednations/cli": "^10.25.13",
    "@unitednations/figma-connect": "^0.0.6",
    "change-case": "^4.1.1",
    "core-js": "^3.16.0",
    "fs-extra": "^8.1.0",
    "js-yaml": "^3.13.0",
    "node-sass": "^6.0.1",
    "rimraf": "^3.0.0"
  },
  "sideEffects": false,
  "gitHead": "1f06c355bfc57d62c4516f9daed57564730537f5"
}
